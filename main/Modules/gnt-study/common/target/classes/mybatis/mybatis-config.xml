<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name = "driver" value = "com.mysql.jdbc.Driver"/>
        <property name = "url" value = "jdbc:mysql://localhost:3303/cible"/>
        <property name="username" value="tester"/>
        <property name="password" value="passwd"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
      <mapper resource="com/huawei/cible/deployservice/db/xml/extend/HostGroupMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/extend/PackageGroupExtMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/extend/DeploymentTokenExtMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/extend/DeploymentExtMapper.xml"/>
      <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentBeanMapper.xml"/>
      <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentGroupBeanMapper.xml"/>
      <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentGroupMappingBeanMapper.xml"/>
      <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentHistoryBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentPolicyBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentPolicyDependencyBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentTemplateBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/DeploymentUnitBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/HostBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/HostDeploymentBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/HostDeploymentTokenBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/HostTagBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/PackageBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/PackageGroupBeanMapper.xml"/>
	  <mapper resource="com/huawei/cible/deployservice/db/xml/PackageGroupMappingBeanMapper.xml"/>  
	  <mapper resource="com/huawei/cible/deployservice/db/xml/TagTargetMappingBeanMapper.xml"/>  
  </mappers>
</configuration>
